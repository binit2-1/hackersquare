{
  "name": "hackersquare",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:up": "docker-compose -f db.docker-compose.yaml up -d",
    "db:down": "docker-compose -f db.docker-compose.yaml down",
    "db:it": "docker exec -it postgres psql -U hackersquare-admin -d hackersquare-dev-db"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.10",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
